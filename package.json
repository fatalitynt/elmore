{
  "name": "my_elmore",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://fatalitynt.github.io/elmore",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "web-vitals": "^2.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "mv-to-docs": "node -e \"require('fs').rename('build', 'docs', function(err) { if (err) console.log(err); else console.log('Done!') })\"",
    "clean-docs": "node -e \"fs.rmdirSync('docs', { recursive: true })\"",
    "deploy": "gh-pages -d build && npm run clean-docs && npm run mv-to-docs"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^3.2.3"
  }
}
